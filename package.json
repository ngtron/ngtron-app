{
    "name": "ngtron",
    "version": "1.0.0",
    "description": "Electron Framework made with Angular 5",
    "main": "dist/main.js",
    "scripts": {
        "electron": "electron dist/main.js",
        "ng": "ng",
        "post": "node post-build.js",
        "start": "ng serve",
        "build": "ng build --prod ---watch",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "rebuild-lib": "electron-rebuild -w sqlite3 -p",
        "watch": "concurrently \"node watch-electron.js\" \"ng build --base-href $(pwd)/dist/app/index.html --watch --stats-json --output-path dist/app\"",
        "watch-and-run": "concurrently \"node watch-electron.js\" \"ng build --base-href $(pwd)/dist/app/index.html --watch --stats-json --output-path dist/app\" \"npm run electron\""
    },
    "author": "Guilherme Fontenele <https://github.com/fontenele>",
    "license": "MIT",
    "dependencies": {
        "@angular/animations": "^5.2.9",
        "@angular/cdk": "^5.2.4",
        "@angular/common": "^5.2.0",
        "@angular/compiler": "^5.2.0",
        "@angular/core": "^5.2.0",
        "@angular/forms": "^5.2.0",
        "@angular/http": "^5.2.0",
        "@angular/material": "^5.2.4",
        "@angular/platform-browser": "^5.2.0",
        "@angular/platform-browser-dynamic": "^5.2.0",
        "@angular/router": "^5.2.0",
        "body-parser": "^1.18.2",
        "chalk": "^2.3.2",
        "chart.js": "^2.7.2",
        "concurrently": "^3.5.1",
        "core-js": "^2.4.1",
        "electron": "^2.0.0-beta",
        "express": "^4.16.3",
        "hammerjs": "^2.0.8",
        "ng-socket-io": "^0.2.4",
        "ng2-charts": "^1.6.0",
        "rxjs": "^5.5.6",
        "socket.io": "^2.1.0",
        "sqlite3": "^4.0.0",
        "zone.js": "^0.8.19"
    },
    "devDependencies": {
        "@angular/cli": "~1.7.4",
        "@angular/compiler-cli": "^5.2.0",
        "@angular/language-service": "^5.2.0",
        "@types/electron": "^1.6.10",
        "@types/jasmine": "~2.8.3",
        "@types/jasminewd2": "~2.0.2",
        "@types/node": "^6.0.105",
        "@types/sqlite3": "^3.1.3",
        "codelyzer": "^4.0.1",
        "electron-rebuild": "^1.7.3",
        "electron-reload": "^1.2.2",
        "jasmine-core": "~2.8.0",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~2.0.0",
        "karma-chrome-launcher": "~2.2.0",
        "karma-coverage-istanbul-reporter": "^1.2.1",
        "karma-jasmine": "~1.1.0",
        "karma-jasmine-html-reporter": "^0.2.2",
        "protractor": "~5.1.2",
        "ts-node": "~4.1.0",
        "tslint": "~5.9.1",
        "typescript": "~2.5.3"
    },
    "engines": {
        "node": ">= 8.0.0",
        "npm": ">= 3"
    }
}
